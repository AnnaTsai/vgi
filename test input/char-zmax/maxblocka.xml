
<fsmxml xmlns="http://vaucanson.lrde.epita.fr" version="1.0">

  <automaton>
    <valueType>
      <writingData closePar=")" openPar="(" plusSym="+" spacesSym=" " starSym="*" timesSym="." weightClosing="}" weightOpening="{" zeroSym="0"/>
      <semiring operations="maxPlus" set="Z" type="numerical"/>
      <monoid genDescrip="enum" genKind="simple" genSort="letters" type="free">
        <writingData identitySym="1"/>
        <monGen value="a"/>
        <monGen value="b"/>
      </monoid>
    </valueType>
    <automatonStruct>
      <states>
        <state id="s0"/>
        <state id="s1"/>
        <state id="s2"/>
      </states>
      <transitions>
        <transition source="s0" target="s0">
          <label>
            <sum>
              <monElmt>
                <monGen value="a"/>
              </monElmt>
              <monElmt>
                <monGen value="b"/>
              </monElmt>
            </sum>
          </label>
        </transition>
        <transition source="s0" target="s1">
          <label>
            <monElmt>
              <monGen value="b"/>
            </monElmt>
          </label>
        </transition>
        <transition source="s1" target="s1">
          <label>
            <leftExtMul>
              <weight value="1"/>
              <monElmt>
                <monGen value="a"/>
              </monElmt>
            </leftExtMul>
          </label>
        </transition>
        <transition source="s1" target="s2">
          <label>
            <monElmt>
              <monGen value="b"/>
            </monElmt>
          </label>
        </transition>
        <transition source="s2" target="s2">
          <label>
            <sum>
              <monElmt>
                <monGen value="a"/>
              </monElmt>
              <monElmt>
                <monGen value="b"/>
              </monElmt>
            </sum>
          </label>
        </transition>
        <initial state="s0"/>
        <initial state="s1"/>
        <final state="s1"/>
        <final state="s2"/>
      </transitions>
    </automatonStruct>
  </automaton>

</fsmxml>
