
<fsmxml xmlns="http://vaucanson.lrde.epita.fr" version="1.0">

  <automaton>
    <valueType>
      <writingData closePar=")" openPar="(" plusSym="+" spacesSym=" " starSym="*" timesSym="." weightClosing="}" weightOpening="{" zeroSym="0"/>
      <semiring operations="maxPlus" set="Z" type="numerical"/>
      <monoid genDescrip="enum" genKind="simple" genSort="letters" type="free">
        <writingData identitySym="1"/>
        <monGen value="a"/>
        <monGen value="b"/>
      </monoid>
    </valueType>
    <automatonStruct>
      <states>
        <state id="s0">
                    <geometricData x="150.0" y="250.0"/>
                </state>
        <state id="s1">
                    <geometricData x="150.0" y="50.0"/>
                </state>
      </states>
      <transitions>
        <transition source="s0" target="s0">
          <label>
            <sum>
              <leftExtMul>
                <weight value="1"/>
                <monElmt>
                  <monGen value="a"/>
                </monElmt>
              </leftExtMul>
              <monElmt>
                <monGen value="b"/>
              </monElmt>
            </sum>
          </label>
        </transition>
        <transition source="s1" target="s1">
          <label>
            <sum>
              <monElmt>
                <monGen value="a"/>
              </monElmt>
              <leftExtMul>
                <weight value="1"/>
                <monElmt>
                  <monGen value="b"/>
                </monElmt>
              </leftExtMul>
            </sum>
          </label>
        </transition>
        <initial state="s0"/>
        <initial state="s1"/>
        <final state="s0"/>
        <final state="s1"/>
      </transitions>
    </automatonStruct>
  </automaton>

</fsmxml>
