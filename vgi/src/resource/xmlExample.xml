<?xml version="1.0" encoding="UTF-8"?>

<fsmxml	xmlns = "http://vaucanson-project.org"
			version = "1.0" >

<automaton name = "a1" >
	<valueType>
		<semiring   type        = 'numerical'
						set         = 'B'
						operations  = 'classical' />
		<monoid     type        = 'free'
						genSort     = 'simple'
						genKind     = 'letter'
						genDescript = 'enum' >
			<monGen	value = "a"/>
			<monGen	value = "b"/>
		</monoid>
	</valueType>
	
	<automatonStruct>
		<states>
			<state	id="0"	name="p">
				<geometricData	x = "10"
									y = "10" />
			</state>
			<state	id="1"	name="q">
				<geometricData	x = "20"
									y = "10" />
				</state>
			<state	id="2"	name="r">
				<geometricData	x = "30"
									y = "10" />
			</state>
		</states>
		<transitions>
			<transition	source="0"	target="0" >
				<label>
					<monElmt>
						<monGen	value="a"/>
					</monElmt>
				</label>
			</transition>
			<transition	source="0"	target="0" >
				<label>
					<monElmt>
						<monGen	value="b"/>
					</monElmt>
				</label>
			</transition>
			<transition	source="0"	target="1" >
				<label>
					<monElmt>
						<monGen	value="a"/>
					</monElmt>
				</label>
			</transition>
			<transition	source="1"	target="2" >
				<label>
					<monElmt>
						<monGen	value="b"/>
					</monElmt>
				</label>
			</transition>
			<transition	source="2"	target="2" >
				<label>
					<monElmt>
						<monGen	value="a"/>
					</monElmt>
				</label>
			</transition>
			<transition	source="2"	target="2" >
				<label>
					<monElmt>
						<monGen	value="b"/>
					</monElmt>
				</label>
			</transition>
			<initial	state="0"/>
			<final	state="2"/>
		</transitions>
	</automatonStruct>
</automaton>

</fsmxml>

